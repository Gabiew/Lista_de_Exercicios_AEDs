Linguagem C

#include <stdio.h>
int main (void) {
 float nota;

printf("Digite a nota do aluno: ");
scanf("%f", &nota);
  
 if (nota >= 10) {
  printf ("Aprovado");
 } else {
   printf ("Reprovado");
 }
 return 0;
}

Portugol//Pseudocódigo

algoritmo "joão_Nota"
